				"foreground": "#000080"
			}
		},
		{
			"scope": "constant.language",//??
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "constant.regexp",//??
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",//??
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "entity.name.tag.css",//??
			"settings": {
				"foreground": "#d7ba7d"
			}
